        "helmet": "^7.0.0",
        "joi": "^17.9.2",
        "jsonwebtoken": "^9.0.1",
        "module-alias": "^2.2.3",
        "redis": "^4.6.7"
    },
    "devDependencies": {
        "@prisma/client": "^5.1.1",
        "@types/bcrypt": "^5.0.0",
        "@types/bcryptjs": "^2.4.2",
